<%= simple_form_for(@creature) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :popular_name %>
    <%= f.input :scientific_name %>
    <%= f.input :place_of_origin %>
    <%= f.input :characteristic %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
