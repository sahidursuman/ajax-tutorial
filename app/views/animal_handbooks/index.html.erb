<h1>Listing animal_handbooks</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @animal_handbooks.each do |animal_handbook| %>
  <tr>
    <td><%= animal_handbook.name %></td>
    <td><%= link_to 'Show', animal_handbook %></td>
    <td><%= link_to 'Edit', edit_animal_handbook_path(animal_handbook) %></td>
    <td><%= link_to 'Destroy', animal_handbook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Animal handbook', new_animal_handbook_path %>
