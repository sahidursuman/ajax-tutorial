<table class="table table-striped table-bordered">
  <tr>
    <th><%= AnimalHandbook.human_attribute_name :name %></th>
    <th><%= t('helpers.actions') %></th>
  </tr>
  <% animal_handbooks.each do |animal_handbook| %>
    <tr>
      <td><%= animal_handbook.name %></td>
      <td class="btn-group">
        <%= link_to_show animal_handbook_path(animal_handbook) %>
        <%= link_to_edit edit_animal_handbook_path(animal_handbook) %>
        <%= link_to_destroy animal_handbook_path(animal_handbook) %>
      </td>
    </tr>
  <% end %>
</table>
