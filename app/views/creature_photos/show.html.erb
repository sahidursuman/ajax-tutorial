<article>
  <header>
    <h1><%= @creature_photo.file_name %></h1>
  </header>

  <%= render_list class: 'nav nav-tabs' do |li|
    li << [link_to(t('helpers.index'), creature_photos_path), { class: 'action' }]
    li << [link_to(t('helpers.edit'), [:edit, @creature_photo]), { class: 'action' }]
  end %>

  <div>
    <%= render 'creature_photo', creature_photo: @creature_photo %>
  </div>

  <footer>
  </footer>
</article>
