<article id="creature-photo-list">
  <header>
    <h1><%= t('.title') %></h1>
  </header>

  <%= render_list class: 'nav nav-tabs' do |li|
    li << [link_to_open_modal(t('helpers.new'), new_creature_photo_path, '#creature-photo-modal'), { class: 'action' }]
  end %>

  <%= render 'table', creature_photos: @creature_photos %>

  <nav role="pagination">
  </nav>
</article>

<div class="modal hide fade" id="creature-photo-modal"></div>
